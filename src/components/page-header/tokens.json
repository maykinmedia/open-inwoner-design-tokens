{
  "oi": {
    "page-header": {
      "$extensions": {
        "dte.metadata": {
          "groupDescription": "Color/spacing tokens are DEPRECATED - use utrecht-page-header tokens instead."
        }
      },

      "bg": {"value": "#ffffff"},
      "mobile": {
        "padding": {"value": "10px 15px 10px 10px"}
      },
      "tablet": {
        "padding": {"value": "{oi.text.tablet.margin}"}
      },
      "laptop": {
        "padding": {"value": "{oi.text.laptop.margin}"}
      },
      "desktop": {
        "padding": {"value": "{oi.text.desktop.margin}"}
      },

      "logo-return-url": {
        "$extensions": {
          "dte.metadata": {
            "groupDescription": "Open Forms extension for logo in the header."
          }
        },

        "min-height": {
          "value": "50px",
          "comment": "DEPRECATED - use min-block-size instead."
        },
        "min-width": {
          "value": "100px",
          "comment": "DEPRECATED - use min-inline-size instead."
        },

        "min-block-size": {"value": "{oi.page-header.logo-return-url.min-height}"},
        "min-inline-size": {"value": "{oi.page-header.logo-return-url.min-width}"},

        "mobile": {
          "min-height": {"value": "25px"},
          "min-width": {"value": "50px"},

          "min-block-size": {"value": "{oi.page-header.logo-return-url.mobile.min-height}"},
          "min-inline-size": {"value": "{oi.page-header.logo-return-url.mobile.min-width}"}
        }
      }
    }
  }
}
